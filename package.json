{
    "name": "flyleaf",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "dev": "nuxt",
        "build": "nuxt build",
        "start": "nuxt start",
        "generate": "nuxt generate",
        "lint:js": "eslint --ext .js,.ts,.vue --ignore-path .gitignore .",
        "lint:style": "stylelint **/*.{vue,css} --ignore-path .gitignore",
        "lint": "npm run lint:style && npm run lint:js",
        "fullup": "rm -rf package-lock.json node_modules/ && npm i && npm run init:mocks",
        "init:mocks": "npx msw init ./static"
    },
    "dependencies": {
        "@nuxtjs/apollo": "^4.0.1-rc.5",
        "@nuxtjs/pwa": "^3.3.5",
        "@nuxtjs/sentry": "^5.0.2",
        "@nuxtjs/svg-sprite": "^0.5.2",
        "@nuxtjs/toast": "^3.3.1",
        "@sentry/tracing": "^6.2.1",
        "core-js": "^3.9.1",
        "graphql-tag": "^2.11.0",
        "msw": "^0.27.1",
        "nuxt": "^2.15.3",
        "nuxt-i18n": "^6.20.6"
    },
    "devDependencies": {
        "@commitlint/cli": "^11.0.0",
        "@commitlint/config-conventional": "^11.0.0",
        "@nuxt/types": "^2.15.2",
        "@nuxt/typescript-build": "^2.1.0",
        "@nuxtjs/color-mode": "^2.0.4",
        "@nuxtjs/eslint-config": "^6.0.0",
        "@nuxtjs/eslint-config-typescript": "^6.0.0",
        "@nuxtjs/eslint-module": "^3.0.2",
        "@nuxtjs/stylelint-module": "^4.0.0",
        "babel-eslint": "^10.1.0",
        "eslint": "^7.21.0",
        "eslint-config-prettier": "^8.1.0",
        "eslint-plugin-no-use-extend-native": "^0.5.0",
        "eslint-plugin-nuxt": "^2.0.0",
        "eslint-plugin-optimize-regex": "^1.2.0",
        "eslint-plugin-prettier": "^3.3.1",
        "eslint-plugin-promise": "^4.3.1",
        "eslint-plugin-sonarjs": "^0.6.0",
        "fibers": "^5.0.0",
        "husky": "^4.3.0",
        "lint-staged": "^10.4.0",
        "postcss-normalize": "^9.0.0",
        "postcss-preset-env": "^6.7.0",
        "prettier": "^2.2.1",
        "sass": "^1.32.5",
        "sass-loader": "^10.1.1",
        "stylelint": "^13.12.0",
        "stylelint-config-prettier": "^8.0.2",
        "stylelint-config-standard": "^21.0.0",
        "stylelint-order": "^4.1.0",
        "stylelint-order-config-yandex": "^0.1.2",
        "stylelint-scss": "^3.19.0"
    },
    "msw": {
        "workerDirectory": "static"
    },
    "lint-staged": {
        "*.{js,ts,vue}": "eslint",
        "*.{css,vue}": "stylelint"
    },
    "husky": {
        "hooks": {
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
            "pre-commit": "lint-staged"
        }
    }
}
